checkm(){
export GPU_FIRST_NVIDIA=`lspci | grep -E "VGA|3D controller" | head -n 1 | grep NVIDIA | wc -l`
if [[ $GPU_FIRST_NVIDIA == 0 ]]; then
    warnmsg=$(cat <<WARN
${BY}
##############################################################################
###									   									   ###
### System has detected that you have connected monitor to integrated GPU  ###
### This is not recommended please connect monitor to first external GPU   ###
### located on the first x16 slot nearest to the CPU. Thank You			   ###
###						System Will Continue in 5 Seconds.				   ###
###									   									   ###
##############################################################################
${N}
WARN
)

	rm -f /etc/X11/xorg.conf

    echo -e "$warnmsg"
    sleep 5
fi
}
